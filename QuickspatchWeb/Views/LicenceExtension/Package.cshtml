    <script type="text/javascript">
        function choosePaymentType(type) {
            if (type == 1) {
                $('#choose-monthly').addClass('chosee-type'); //
                $('#choose-annually').removeClass('chosee-type');
                $('#monthly').show();
                $('#annually').hide();
                $('#discount-lable').hide();
            };
            if (type == 2) {
                $('#choose-monthly').removeClass('chosee-type');
                $('#choose-annually').addClass('chosee-type'); //
                $('#monthly').hide();
                $('#annually').show();
                $('#discount-lable').show();
            };
        }
        var packageId = 0;
        function active() {
            //$.get("/LicenceExtension/GetDescripttionPackage", { packageId: packageId }, function(result) {
            //    $('#enterprise-btn, #standard-btn, #premium-btn').prop('disabled', true);
            //    bootbox.confirm("Are you sure select <strong>" + result + "</strong>?", function(result) {
            //        if (result) {
            $.get("/LicenceExtension/Active", { keyCode: '@Model.KeyCode', packageId: packageId }, function(result) {
                window.location.href = result.PaymentUrl + '?data=' + result.Data;
            }).always(function() {
                $('#enterprise-btn, #standard-btn, #premium-btn').prop('disabled', true);
            });
            //        } else {
            //            $('#enterprise-btn, #standard-btn, #premium-btn').prop('disabled', false);
            //        }
            //    });
            //});
        };

        function showContactUs() {
            window.location.href = "/LicenceExtension/ContactUs?keyCode=@Model.KeyCode";
        };

        //CSS  choose package
        function removeCss() {
            $('#package-standard').removeClass('package-old'); //
            $("#button-standard").html('Choose plan');   //

            $('#package-prenium').removeClass('package-old');
            $("#button-prenium").html('Choose plan');

            $('#package-enterprise').removeClass('package-old');
            $("#button-enterprise").html('Choose plan');

            $('#package-enterprise-plus').removeClass('package-old');
            $("#button-enterprise-plus").html('Choose plan');

            $('#package-standard-annually').removeClass('package-old');
            $("#button-standard-annually").html('Choose plan');

            $('#package-prenium-annually').removeClass('package-old');
            $("#button-prenium-annually").html('Choose plan');

            $('#package-enterprise-annually').removeClass('package-old');
            $("#button-enterprise-annually").html('Choose plan');

            $('#package-enterprise-plus-annually').removeClass('package-old');
            $("#button-enterprise-plus-annually").html('Choose plan');
        }
        function selectStandard() {
            packageId = 2;
            removeCss();
            $('#package-standard').addClass('package-old'); //
            $("#button-standard").html('Selected');   //
        };

        function selectStandardAnnually() {
            packageId = 1;
            removeCss();
            $('#package-standard-annually').addClass('package-old'); //
            $("#button-standard-annually").html('Selected');  //
        };

        function selectPreniumAnnually() {
            packageId = 3;
            removeCss();
            $('#package-prenium-annually').addClass('package-old'); //
            $("#button-prenium-annually").html('Selected');    //
        };

        function selectPrenium() {
            packageId = 4;
            removeCss();
            $('#package-prenium').addClass('package-old');//
            $("#button-prenium").html('Selected');  //
        };

        function selectEnterpriseAnnually() {
            packageId = 5;
            removeCss();
            $('#package-enterprise-annually').addClass('package-old'); //
            $("#button-enterprise-annually").html('Selected'); //
        };

        function selectEnterprise() {
            packageId = 6;
            removeCss();
            $('#package-enterprise').addClass('package-old'); //
            $("#button-enterprise").html('Selected');    //
        };

        function selectEnterprisePlusAnnually() {
            packageId = 7;
            removeCss();
            $('#package-enterprise-plus-annually').addClass('package-old');//
            $("#button-enterprise-plus-annually").html('Selected');  //
        };

        function selectEnterprisePlus() {
            packageId = 8;
            removeCss();
            $('#package-enterprise-plus').addClass('package-old');//
            $("#button-enterprise-plus").html('Selected'); //
        };

        //show package month
        function showPackageMonthly() {
            $('#choose-monthly').addClass('chosee-type'); //
            $('#choose-annually').removeClass('chosee-type');
            $('#monthly').show();
            $('#annually').hide();
            $('#discount-lable').hide();
        };

        //show package year
        function showPackageAnnually() {
            $('#choose-monthly').removeClass('chosee-type'); //
            $('#choose-annually').addClass('chosee-type');
            $('#monthly').hide();
            $('#annually').show();
            $('#discount-lable').show();
        };

        $(document).ready(function () {

            $('#re-active').click(function () {
                $('#re-active').prop('disabled', true);

            });
            var currentPackage =  @Model.PackageId;
            if (currentPackage != 0 && currentPackage % 2 == 0) {
                showPackageMonthly();
            }
            else if (currentPackage != 0 && currentPackage % 2 != 0) {
                showPackageAnnually();
            }
            else {
                showPackageAnnually();
            }

            packageId = currentPackage == 0 ? 1 : currentPackage;

            switch (currentPackage) {
                case 1:
                    $('#package-standard-annually').addClass('package-old'); //
                    $("#button-standard-annually").html('Selected');   //
                    break;
                case 2:
                    $('#package-standard').addClass('package-old'); //
                    $("#button-standard").html('Selected');   //
                    break;
                case 3:
                    $('#package-prenium-annually').addClass('package-old'); //
                    $("#button-prenium-annually").html('Selected');   //
                    break;
                case 4:
                    $('#package-prenium').addClass('package-old'); //
                    $("#button-prenium").html('Selected');   //
                    break;
                case 5:
                    $('#package-enterprise-annually').addClass('package-old'); //
                    $("#button-enterprise-annually").html('Selected');   //
                    break;
                case 6:
                    $('#package-enterprise').addClass('package-old'); //
                    $("#button-enterprise").html('Selected');   //
                    break;
                case 7:
                    $('#package-enterprise-plus-annually').addClass('package-old'); //
                    $("#button-enterprise-plus-annually").html('Selected');   //
                    break;
                case 8:
                    $('#package-enterprise-plus').addClass('package-old'); //
                    $("#button-enterprise-plus").html('Selected');   //
                    break;

                default:
                    $('#package-standard-annually').addClass('package-old'); //
                    $("#button-standard-annually").html('Selected');   //
                    break;
            }
        });

    </script>

