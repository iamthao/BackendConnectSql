@{
    Layout = null;
}

@model QuickspatchWeb.Models.FranchiseeConfiguration.LicenceExtensionData


<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<!-- Mirrored from themes.3rdwavemedia.com/tempo/tour.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 07 Aug 2015 07:11:21 GMT -->
<head>
    <title>QuickSpatch - Camino Information Services</title>

    <!-- Meta -->
    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,300italic,400italic' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link rel="icon" href="/content/quickspatch/img/favicon.ico" type="image/x-icon">
    <!-- Global CSS -->
    <link rel="stylesheet" href="@(Url.Content("~/Content/quickspatch/css/bootstrap/bootstrap.min.css"))">
    <!-- Plugins CSS -->
    <link href="~/content/quickspatch/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Theme CSS -->
    <link rel="stylesheet" href="@(Url.Content("~/Content/quickspatch/css/bootstrap-tour.min.css"))">
    <link rel="stylesheet" href="@(Url.Content("~/Content/quickspatch/css/wellcome.css"))">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <style>
        .navbar {
            display: none;
        }

        .popover {
            z-index: 9999 !important;
            border: 1px solid #d90007;
        }

        .popover-title {
            background: #d90007;
            color: #fff;
            font-weight: bold;
        }

        .popover.top > .arrow {
            border-top-color: #d90007;
        }

        .btn-activate {
            background: #008de7;
            color: #ffffff;
            border: 1px #008de7 solid;
            -webkit-border-radius: 2px;
        }

            .btn-activate:hover, .btn-activate:active, .btn-activate:focus {
                background: #005e9b;
                color: #fff;
                border: 1px #005e9b solid;
                -webkit-border-radius: 2px;
            }
    </style>
    <script type="text/javascript" src="~/Scripts/jquery-2.0.3.min.js"></script>
    <script type="text/javascript" src="~/Scripts/angularApp/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/quickspatch/bootstrap-tour.min.js"></script>
</head>
<body class="tour-page" ng-app="app">
    <div class="wrapper">
        <!-- ******HEADER****** -->
        <header id="header" class="header">
            <div class="container">
                <h1 class="logo pull-left">
                    <a href="javascript:void(0)">
                        <span class="logo-title"><img src="@(Url.Content("~/Content/quickspatch/img/logo-o.svg"))" alt="QuickSpatch" height="40" /></span>
                    </a>
                </h1><!--//logo-->
                <nav id="main-nav" class="main-nav navbar-right" role="navigation">
                    <div class="navbar-header">
                        <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button><!--//nav-toggle-->
                    </div><!--//navbar-header-->
                </nav><!--//main-nav-->
            </div><!--//container-->
        </header><!--//header-->
        <!-- ******FEATURES PROMO****** -->
        <section id="features-promo" class="features-promo section video" style="min-height: 100px">
            <div class="container" style="color: white">
                <h2 class="tile">Welcome back to QuickSpatch.</h2>
            </div><!--//container-->
        </section><!--//features-promo-->
        <section id="features" class="features">
            <div class="container">
                <div class="row">
                    <div class="content col-md-12 col-sm-12 col-xs-12" style="margin-left: 30px;">
                        <strong>Dear @Model.UserName !</strong>
                        <br />
                        <br />
                        <span>Your account has been expried. Let click Active to continue using QuickSpatch.</span>
                        <br />
                        <br />
                        <button class="btn btn-primary btn-activate" id="re-active">Activate</button>
                    </div>
                </div>
            </div><!--//container-->
        </section><!--//features-promo-->



    </div><!--//wrapper-->
    <script type="text/javascript">
        $(document).ready(function () {
            $('#re-active').click(function () {
                $('#re-active').prop('disabled', true);

                @*$.get("/LicenceExtension/Active", { keyCode: '@Model.KeyCode' }, function (result) {
                            window.location.href = result.PaymentUrl + '?data=' + result.Data;
                        }).always(function () {
                            $('#re-active').prop('disabled', false);
                        });*@
                window.location.href = "/LicenceExtension/Package?keyCode=@Model.KeyCode";
                //$('#re-active').prop('disabled', false);
            });
        });

    </script>

</body>
</html>