@model Framework.Exceptions.UnAuthorizedAccessException


@{
    Layout = null;
}

<!DOCTYPE html>

<html>

    <head>
        <title>Error - UnAuthorized Access</title>

        <style type="text/css">
            body {
                font-family: "Segoe UI Light", "Lucida Grande", Verdana, Arial, Helvetica, sans-serif;
            }

            h3 {
                color: #00B0F0;
                font-size: 1.4em;
                margin: 10px 0 0 0;
            }
        </style>
    </head>
    <body style="padding: 0; margin: 0; width: 100%; height: 100%; background: url('/Content/quickspatch/img/error/bg_error.png') repeat; text-align: center;">
        <img src="@Url.Content("~/Content/quickspatch/img/error/no-permission.png")" alt="" />
       
        @if (Model != null)
        {
            <center><small><p class="text-error">Error: @Model.Message</p></small></center>
        }
    </body>
</html>
