@using QuickspatchWeb.Models
@using QuickspatchWeb.Models.FranchiseeTenant
@model DashboardFranchiseeTenantDataViewModel
@{
    Layout = "~/Views/Shared/_LayoutPopup.cshtml";
}

<div ng-controller="updateFranchiseeController">
    @Html.Partial("_Shared", Model.SharedViewModel)
</div>

@section FooterActions
{
    @Model.AddFooterAction("Update", "fa-save", FooterActionEnum.Save, "updateFranchiseeController")
    @Model.AddFooterAction("Cancel", "fa-close", FooterActionEnum.Cancel, "updateFranchiseeController", false)

    @Html.Partial("~/Views/Shared/_FooterActions.cshtml", Model.FooterActions)
}
