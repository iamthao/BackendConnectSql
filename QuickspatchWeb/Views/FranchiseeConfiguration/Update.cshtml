@using QuickspatchWeb.Models
@using QuickspatchWeb.Models.FranchiseeConfiguration
@model DashboardFranchiseeConfigurationDataViewModel
@{
    Layout = "~/Views/Shared/_LayoutPopup.cshtml";
}

<div ng-controller="updateFranchiseeConfigurationController">
    @Html.Partial("_Shared", Model.SharedViewModel)
</div>

@section FooterActions
{
    @Model.AddFooterAction("Update", "fa-save", FooterActionEnum.Save, "updateFranchiseeConfigurationController")
    @Model.AddFooterAction("Cancel", "fa-close", FooterActionEnum.Cancel, "updateFranchiseeConfigurationController", false)

    @Html.Partial("~/Views/Shared/_FooterActions.cshtml", Model.FooterActions)
}
