    <script>
        $(document).ready(function () {
            $("body").css({ height: $(window).height() });
            $("#main").css({ "padding": "0", "min-height": "auto", "padding-top": ($(window).height() - 490) / 2, "width": $(window).width() > 400 ? "400px" : $(window).width() });

            resetCenterPosition();
        });

        function resetCenterPosition() {
            $("#sign-in-form").css({ "top": (window.innerHeight - $("#sign-in-form").height()) / 2 - 40 + "px", "left": $(window).width() > 400 ? (window.innerWidth - $("#sign-in-form").width()) / 2 + "px" : 0, "width": $(window).width() > 400 ? "400px" : $(window).width() + "px" });

        }

        $("#sign-in-button").css({ display: "block" });

        $(window).resize(function () {
            resetCenterPosition();
        });
    </script>
